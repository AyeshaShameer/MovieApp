<h1>Movies#index</h1>
<p>Find me in app/views/movies/index.html.erb</p>
<div class="main">
  <div class="container">
  <h2>Popular Films</h2>
    <% @movies.each do |movie| %>
    <div class="movie">
      <%= image_tag movie.image %>
      <h3><%= movie.title %></h3>
      <p><%= movie.release_year %></p>
    </div>
    <% end %>
  </div>
</div>

